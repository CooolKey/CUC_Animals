/* pages/search/search.wxss */
.search-hd {
  width: 100%;
  height: 115rpx;
  background-color: #fff;
  border-bottom: 1px solid #f8f8f8;
}
.search-area {
  box-sizing: border-box;
  position: relative;
  margin: 30rpx;
}

.search-txt {
  box-sizing: border-box;
  width: 100%;
  height: 80rpx;
  padding-left: 30rpx;
  padding-right: 30rpx;
  border-radius: 20rpx;
  border: 1px solid rgb(245, 245, 245);
  font-family: "微软雅黑";
  background-color: rgb(245, 245, 245);
}

.search-btn {
  position: absolute;
  z-index: 11;
  top: 0;
  right: -10rpx;
  height: 80rpx;
  width: 100rpx;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
  background-color: rgba(0, 0, 0, 0);
  background-image: url(data:image/png;base64,);
  background-repeat: no-repeat; 
  background-size: 100% 100%;
  -moz-background-size:100% 100%;
}

button::after{
  border: none;
  
}

.search-btn::after{
  border-bottom-left-radius: 0;
  border-top-left-radius: 0;
}

.search-keyword {
  padding: 50rpx;
}

.search-keyword-title {
  padding-left: 5rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  font-size: 28rpx;
  border-bottom: 0rpx solid rgb(241, 241, 241);
}

.search-keyword-item {
  display: inline-block;
  font-size: 25rpx;
  padding: 10rpx;
  border: 0px solid rgb(245, 245, 245);
  border-radius: 10rpx;
  background-color: rgb(245, 245, 245);
  margin: 0 15rpx 10rpx 0;
  color: #555;
}